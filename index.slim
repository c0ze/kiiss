---
layout: default
---

section.main-content
  article.module.color-3[ng-repeat="feed in feeds"]
    a[ng-href="/feed.html?{{ feed.get('name') }}"]
      h1[ng-bind="feed.get('name')"]
    hr[ng-bind="feed.get('url')"]
    button.btn.btn-danger.pull-right[type="submit" ng-click="delete(feed)"]
      span.glyphicon-trash.glyphicon.glyphicon-white
        |  Delete
  .module.pagination.color-3
    |  {% if paginator.previous_page %} 
    a.previous[href="{{ site.url }}{{ paginator.previous_page_path }}"]
      | Previous
    |  {% endif %} 
    span.page_number
      | — {{ paginator.page }} of {{ paginator.total_pages }} —
    |  {% if paginator.next_page %} 
    a.next[href="{{ site.url }}{{ paginator.next_page_path }}"]
      | Next
    |  {% endif %} 
